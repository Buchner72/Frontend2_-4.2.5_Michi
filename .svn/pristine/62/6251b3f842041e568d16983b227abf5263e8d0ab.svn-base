<div class="navbar-fixed-top" style="margin-top:100px;" *ngIf="percent != 0">
    <div class="row">
        <div class="col-md-12">
            <progressbar class="progress" [max]="100" [value]="percent">
                <span style="color:white; white-space:nowrap;">{{percent}} %</span>
            </progressbar>
        </div>
    </div>
</div>
